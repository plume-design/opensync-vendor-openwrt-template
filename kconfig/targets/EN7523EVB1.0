@include "en7529"
@include "mt7916"
CONFIG_CM2_STABILITY_USE_RESTORE_SWITCH_CFG=y
CONFIG_DHSNIFF_PCAP_BUFFER_SIZE=65536
CONFIG_DHSNIFF_PCAP_SNAPLEN=1024
CONFIG_HOSTAP_PSK_FILE_WPS=y
CONFIG_INET_DNS_RESOLVCONF=y
# CONFIG_INET_GRE_USE_GRETAP is not set
CONFIG_INET_GRE_USE_SOFTWDS=y
CONFIG_INET_SOFTWDS=y
CONFIG_MANAGER_NFM=y
CONFIG_MANAGER_NM_MCAST_EXCEPTIONS=y
CONFIG_MANAGER_WANO_IFACE_LIST="eth0.1 eth0.2 eth0.3 eth0.4"
CONFIG_OOKLA_OPT_GRO=y
CONFIG_OVSDB_BOOTSTRAP=y
