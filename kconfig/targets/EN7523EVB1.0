CONFIG_OVSDB_BOOTSTRAP_WIFI_STA_LIST="phy0:bhaul-sta-24 phy1:bhaul-sta-50"
CONFIG_TARGET_ETH_LIST=y
CONFIG_TARGET_ETH0_LIST=y
CONFIG_TARGET_ETH0_NAME="eth0.1"
CONFIG_TARGET_ETH1_LIST=y
CONFIG_TARGET_ETH1_NAME="eth0.2"
CONFIG_TARGET_ETH2_LIST=y
CONFIG_TARGET_ETH2_NAME="eth0.3"
CONFIG_TARGET_ETH3_LIST=y
CONFIG_TARGET_ETH3_NAME="eth0.4"
CONFIG_WIFI_UNUSED_IFACE_LIST="wlan0 wlan1"
CONFIG_TARGET_MODEL_GET=y
CONFIG_TARGET_MODEL="EN7523EVB1.0"
CONFIG_TARGET_NAME="EN7523EVB1.0"
CONFIG_PM_ENABLE_LM=y
CONFIG_TARGET_OPENSYNC_CAFILE="ca.pem"
